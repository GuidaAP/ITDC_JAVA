import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		
		int min = scanner.nextInt();
		System.out.print("¬ведите минимальное значение? " + min);
		
		int max = scanner.nextInt();
		System.out.print("¬ведите минимальное значение? " + max);
		
		int random_mix = min + (int) (Math.random()*max);
		
//		int random_min = min + (int) (Math.random()*ax);
//		int random_max = min + (int) (Math.random()*max);
		
		System.out.println("—лучайно число от" + min + "до" +  max + ":" + " " + random_mix);
//		System.out.printf("—лучайно число от" + min + "до" +  max + ":" + " " + Math.random(min, max);

	}

}
