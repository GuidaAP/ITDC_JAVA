import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
        int mins = scanner.nextInt();
        
        int digital_time_mins = mins / 60;
        int digital_time_sec  = mins % 60;

        
        System.out.println(digital_time_mins + " " + digital_time_sec);
        
		
		
		
	}

}
